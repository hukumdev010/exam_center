{
  "name": "exam_center_devcontainer",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/exam_center",
  "forwardPorts": [3000, 5432],
  "postCreateCommand": "npm install && npx prisma generate && npx prisma db push && npm run db:seed",
  "mounts": [
    "source=/home/bhabana_basnet/.ssh,target=/home/node/.ssh,type=bind,consistency=cached"
  ],
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "ms-vscode.vscode-typescript-next",
        "bradlc.vscode-tailwindcss",
        "prisma.prisma"
      ]
    }
  },
  "remoteUser": "node"
}
